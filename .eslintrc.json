{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "class-methods-use-this": "off",
        "import/prefer-default-export": "off",
        "import/no-extraneous-dependencies": ["off", {"devDependencies": false }]
    }
}
